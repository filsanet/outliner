<outline text="Front End" isMoveableInherited="true" isCommentInherited="true" isEditableInherited="true">
	<outline text="About JOE - the Java Outline Editor">
		<outline text="What It Is">
			<outline text="JOE is an outliner. "/>
			<outline text="JOE is programmed in Java."/>
		</outline>
		<outline text="Notable Features">
			<outline text="Open Source"/>
			<outline text="Supports Industry-Standard Data Formats">
				<outline text="OPML">
					<outline text="OPML 1.0">
						<outline text="TODO: link to OPML spec"/>
					</outline>
					<outline text="Userland node types and attributes"/>
					<outline text="JOE node types and attributes"/>
					<outline text=""/>
				</outline>
				<outline text="Text "/>
				<outline text="XML"/>
				<outline text="Palm pdf formats [forthcoming]">
					<outline text="Brain Forest"/>
					<outline text="Thought Manager"/>
				</outline>
			</outline>
			<outline text="Net-Friendly">
				<outline text="http support [forthcoming]"/>
				<outline text="ftp support [forthcoming]"/>
				<outline text="WebDav support [forthcoming]"/>
			</outline>
			<outline text="OS Platform-Agnostic">
				<outline text="JOE is programmed in Java, using the Swing UI classes">
					<outline text="JOE supports native platform look and feel [forthcoming when Sun gets this stuff stable]"/>
				</outline>
				<outline text="JOE releases are available for these platforms">
					<outline text="Windows"/>
					<outline text="Linux/Unix [forthcoming]"/>
					<outline text="Macintosh [forthcoming]"/>
				</outline>
			</outline>
			<outline text="Smooth Nimble Solid Feel"/>
			<outline text="Standards-Respectful UI">
				<outline text="Userland-inspired outline-editing UI">
					<outline text="link to Userland outliner site[s]"/>
				</outline>
				<outline text="Best-practices File/Edit/Search/Window/Help menu command sets"/>
			</outline>
			<outline text="Multiple undo">
				<outline text="limit [huge]: available memory"/>
			</outline>
			<outline text="Easy to Customize">
				<outline text="Application Preferences"/>
				<outline text="User Preferences [forthcoming]"/>
				<outline text="Document Preferences"/>
			</outline>
			<outline text="Multi-User Friendly"/>
		</outline>
	</outline>
	<outline text="About JOE User Guide [JUG]">
		<outline text="help !!!!">
			<outline text="This is a work in perpetual progress."/>
			<outline text="Empty andor missing nodes are not bugs, they're just things we just haven't gotten 'round to thinking andor writing about yet"/>
