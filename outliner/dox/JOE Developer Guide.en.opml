<outline isEditable="true" isMoveable="true" text="Setting Up a JOE Development Environment" isMoveableInherited="true" isEditableInherited="true">
	<outline isEditable="true" isMoveable="true" text="Windows">
		<outline isEditable="true" isMoveable="true" text="Front Matter">
			<outline isEditable="true" isMoveable="true" text="This Version">
				<outline isEditable="true" isMoveable="true" text="2001-07-21"/>
			</outline>
			<outline isEditable="true" isMoveable="true" text="Latest Version">
				<outline isEditable="true" isMoveable="true" text="http://StanKrute.com/JOE/Setting Up the JOE Open Source Project on a Windows System.opml"/>
			</outline>
			<outline isEditable="true" isMoveable="true" text="Feedback Invited">
				<outline isEditable="true" isMoveable="true" text="send it to the author, Stan Krute"/>
				<outline isEditable="true" isMoveable="true" text="Stan@StanKrute.com"/>
			</outline>
			<outline isEditable="true" isMoveable="true" text="Copyright ">
				<outline isEditable="true" isMoveable="true" text="This guide [c] 2001 by Stan Krute. All rights reserved."/>
			</outline>
			<outline isEditable="true" isMoveable="true" text="Thanks to">
				<outline isEditable="true" isMoveable="true" text="Dan Weston"/>
				<outline isEditable="true" isMoveable="true" text="Maynard Demmon"/>
				<outline isEditable="true" isMoveable="true" text="Michael Hearn"/>
				<outline isEditable="true" isMoveable="true" text="These Fine Tools &amp; Their Communities">
					<outline isEditable="true" isMoveable="true" text="SourceForge"/>
					<outline isEditable="true" isMoveable="true" text="ssh"/>
					<outline isEditable="true" isMoveable="true" text="cvs"/>
					<outline isEditable="true" isMoveable="true" text="NetBeans"/>
					<outline isEditable="true" isMoveable="true" text="Java"/>
				</outline>
			</outline>
			<outline isEditable="true" isMoveable="true" text="References">
				<outline isEditable="true" isMoveable="true" text="An Introductory Guide to Using SourceForge for Windows Development">
					<outline isEditable="true" isMoveable="true" text="link: http://sourceforge.net/docman/display_doc.php?docid=766&amp;group_id=1"/>
				</outline>
			</outline>
			<outline isEditable="true" isMoveable="true" text="OS assumptions">
				<outline isEditable="true" isMoveable="true" text="You're running a modern Win32 Windows OS">
					<outline isEditable="true" isMoveable="true" text="Win9x: Windows 98SE or later"/>
					<outline isEditable="true" isMoveable="true" text="NT/2K: Windows 2000 or later"/>
				</outline>
			</outline>
			<outline isEditable="true" isMoveable="true" text="Introduction">
				<outline isEditable="true" isMoveable="true" text="Some cookie crumbs for your trail">
					<outline isEditable="true" isMoveable="true" text="I was new to ssh, CVS, SourceForge, Java, Java JDK, NetBeans, and JOE"/>
					<outline isEditable="true" isMoveable="true" text="It took me many hours to get things set up so that I could work on the JOE project"/>
					<outline isEditable="true" isMoveable="true" text="I put together this doc with the hope that it can speed the process for others"/>
				</outline>
				<outline isEditable="true" isMoveable="true" text="This is just one of a large number of paths">
					<outline isEditable="true" isMoveable="true" text="Two salient features:">
						<outline isEditable="true" isMoveable="true" text="It's a path the author's followed"/>
						<outline isEditable="true" isMoveable="true" text="It works"/>
					</outline>
				</outline>
				<outline isEditable="true" isMoveable="true" text="As you travel your own, send us info">
					<outline isEditable="true" isMoveable="true" text="mailto: DocHelp@JavaOutlineEditor.org"/>
				</outline>
			</outline>
		</outline>
		<outline isEditable="true" isMoveable="true" text="Places">
			<outline isEditable="true" isMoveable="true" text="Enroll at SourceForge"/>
			<outline isEditable="true" isMoveable="true" text="Set Up Directories on Local Machine">
				<outline isEditable="true" isMoveable="true" text="home at root of some drive">
					<outline isEditable="true" isMoveable="true" text="mine is c:\home"/>
					<outline isEditable="true" isMoveable="true" text="lots of unixoid programs will store data sub this directory"/>
				</outline>
				<outline isEditable="true" isMoveable="true" text="cvs at root of some drive">
					<outline isEditable="true" isMoveable="true" text="mine is c:\cvs"/>
				</outline>
				<outline isEditable="true" isMoveable="true" text="ssh at root of some drive">
					<outline isEditable="true" isMoveable="true" text="mine is c:\ssh"/>
				</outline>
				<outline isEditable="true" isMoveable="true" text="a project directory somewhere">
					<outline isEditable="true" isMoveable="true" text="mine is at e:\stan's java projects\Joe"/>
				</outline>
			</outline>
			<outline isEditable="true" isMoveable="true" text="Set up CVS directory on SourceForge">
				<outline isEditable="true" isMoveable="true" text="Open up a command prompt"/>
				<outline isEditable="true" isMoveable="true" text="Give this command">
					<outline isEditable="true" isMoveable="true" text="ssh -l &lt;user_name&gt; cvs.sourceforge.net">
						<outline isEditable="true" isMoveable="true" text="ssh -l stanley_krute cvs.sourceforge.net"/>
					</outline>
				</outline>
				<outline isEditable="true" isMoveable="true" text="When prompted, supply your SourceForge password"/>
				<outline isEditable="true" isMoveable="true" text="You'll be disconnected when the setup is done"/>
				<outline isEditable="true" isMoveable="true" text="Sample output">
					<outline isEditable="true" isMoveable="true" text="E:\stan's java projects\Joe\outliner&gt;ssh -l stanley_krute cvs.sourceforge.net"/>
					<outline isEditable="true" isMoveable="true" text="Host 'cvs.sourceforge.net' added to the list of known hosts."/>
					<outline isEditable="true" isMoveable="true" text="Password:"/>
					<outline isEditable="true" isMoveable="true" text="Creating home directory '/home/users/s/st/stanley_krute'."/>
					<outline isEditable="true" isMoveable="true" text="Linux usw-pr-cvs1 2.2.18pre11-va2.0-servers4 #1 SMP Thu Mar 29 20:48:12 PST 2001"/>
					<outline isEditable="true" isMoveable="true" text=" i686 unknown"/>
					<outline isEditable="true" isMoveable="true" text=""/>
					<outline isEditable="true" isMoveable="true" text="Most of the programs included with the Debian GNU/Linux system are"/>
					<outline isEditable="true" isMoveable="true" text="freely redistributable; the exact distribution terms for each program"/>
					<outline isEditable="true" isMoveable="true" text="are described in the individual files in /usr/doc/*/copyright"/>
					<outline isEditable="true" isMoveable="true" text=""/>
					<outline isEditable="true" isMoveable="true" text="Debian GNU/Linux comes with ABSOLUTELY NO WARRANTY, to the extent"/>
					<outline isEditable="true" isMoveable="true" text="permitted by applicable law."/>
					<outline isEditable="true" isMoveable="true" text=""/>
					<outline isEditable="true" isMoveable="true" text="Welcome to cvs.sourceforge.net"/>
					<outline isEditable="true" isMoveable="true" text=""/>
					<outline isEditable="true" isMoveable="true" text="This is a restricted Shell Account"/>
					<outline isEditable="true" isMoveable="true" text="You cannot execute anything here."/>
					<outline isEditable="true" isMoveable="true" text=""/>
					<outline isEditable="true" isMoveable="true" text="Connection to cvs.sourceforge.net closed."/>
					<outline isEditable="true" isMoveable="true" text=""/>
					<outline isEditable="true" isMoveable="true" text="E:\stan's java projects\Joe\outliner&gt;"/>
				</outline>
			</outline>
		</outline>
		<outline isEditable="true" isMoveable="true" text="Tools">
			<outline isEditable="true" isMoveable="true" text="a Programmer's Text Editor">
				<outline isEditable="true" isMoveable="true" text="jEdit"/>
				<outline isEditable="true" isMoveable="true" text="Ultra-Edit 32"/>
			</outline>
			<outline isEditable="true" isMoveable="true" text="a Secure Terminal Shell">
				<outline isEditable="true" isMoveable="true" text="Obtain and Install ssh">
					<outline isEditable="true" isMoveable="true" text="About ssh">
						<outline isEditable="true" isMoveable="true" text="ssh stands for 'secure shell'"/>
						<outline isEditable="true" isMoveable="true" text="it provides a secure means of communication with a server"/>
						<outline isEditable="true" isMoveable="true" text="it is a command line program"/>
					</outline>
					<outline isEditable="true" isMoveable="true" text="Which version to get">
						<outline isEditable="true" isMoveable="true" text="TBD"/>
					</outline>
					<outline isEditable="true" isMoveable="true" text="Where to get it from">
						<outline isEditable="true" isMoveable="true" text="Obtain and Install SourceForge Setup">
							<outline isEditable="true" isMoveable="true" text="It's at TBD"/>
						</outline>
					</outline>
					<outline isEditable="true" isMoveable="true" text="Installation"/>
					<outline isEditable="true" isMoveable="true" text="Run it">
						<outline isEditable="true" isMoveable="true" text=""/>
					</outline>
					<outline isEditable="true" isMoveable="true" text="Choose to download and install ssh">
						<outline isEditable="true" isMoveable="true" text="Don't download and install WinCVS"/>
					</outline>
					<outline isEditable="true" isMoveable="true" text="Log into your project">
						<outline isEditable="true" isMoveable="true" text="ssh -l &lt;user_name&gt; projectname.sourceforge.net">
							<outline isEditable="true" isMoveable="true" text="ssh -l stanley_krute outliner.sourceforge.net"/>
						</outline>
					</outline>
					<outline isEditable="true" isMoveable="true" text="Log out">
						<outline isEditable="true" isMoveable="true" text="exit"/>
					</outline>
				</outline>
			</outline>
			<outline isEditable="true" isMoveable="true" text="a Version Control System">
				<outline isEditable="true" isMoveable="true" text="Obtain and Install CVS">
					<outline isEditable="true" isMoveable="true" text="Why CVS ?"/>
					<outline isEditable="true" isMoveable="true" text="Download it ">
						<outline isEditable="true" isMoveable="true" text="Get it from http://cvshome.org"/>
					</outline>
					<outline isEditable="true" isMoveable="true" text="installation notes">
						<outline isEditable="true" isMoveable="true" text="suggestions">
							<outline isEditable="true" isMoveable="true" text="Install in a top-level directory">
								<outline isEditable="true" isMoveable="true" text="e:\cvs"/>
							</outline>
						</outline>
						<outline isEditable="true" isMoveable="true" text="adjust autoexec.bat">
							<outline isEditable="true" isMoveable="true" text="add cvs install directory to path"/>
							<outline isEditable="true" isMoveable="true" text="set CVSROOT">
								<outline isEditable="true" isMoveable="true" text="set CVSROOT to point to the reposity, which in this case resides out on SourceForge"/>
								<outline isEditable="true" isMoveable="true" text="set CVSROOT=:pserver:anonymous@cvs.outliner.sourceforge.net:/cvsroot/outliner"/>
							</outline>
						</outline>
					</outline>
					<outline isEditable="true" isMoveable="true" text="Sample CVS Commands"/>
				</outline>
			</outline>
			<outline isEditable="true" isMoveable="true" text="a Development Kit">
				<outline isEditable="true" isMoveable="true" text="Obtain and Install Java JDK">
					<outline isEditable="true" isMoveable="true" text="Why Java ?"/>
					<outline isEditable="true" isMoveable="true" text="Download it">
						<outline isEditable="true" isMoveable="true" text="Latest version:"/>
						<outline isEditable="true" isMoveable="true" text="Available at:"/>
					</outline>
					<outline isEditable="true" isMoveable="true" text="installation notes"/>
				</outline>
			</outline>
			<outline isEditable="true" isMoveable="true" text="an Integrated Development Environment [IDE]">
				<outline isEditable="true" isMoveable="true" text="NetBeans">
					<outline isEditable="true" isMoveable="true" text="Obtain and Install NetBeans IDE">
						<outline isEditable="true" isMoveable="true" text="NetBeans">
							<outline isEditable="true" isMoveable="true" text="Why NetBeans ?"/>
						</outline>
						<outline isEditable="true" isMoveable="true" text="Download it">
							<outline isEditable="true" isMoveable="true" text="Latest version:"/>
							<outline isEditable="true" isMoveable="true" text="Available at:"/>
						</outline>
						<outline isEditable="true" isMoveable="true" text="basic installation notes">
							<outline isEditable="true" isMoveable="true" text="Double-click the download .exe file to run the NetBeans 3.2 Installer">
								<outline isEditable="true" isMoveable="true" text="It should find the Java JDK installation">
									<outline isEditable="true" isMoveable="true" text="If it doesn't, there's something wrong with your JDK installation, so abort, and re-install the JDK"/>
								</outline>
							</outline>
							<outline isEditable="true" isMoveable="true" text="run the Initial NetBeans startup">
								<outline isEditable="true" isMoveable="true" text="Do you need to import settings ?">
									<outline isEditable="true" isMoveable="true" text="Default is no, that's fine"/>
								</outline>
								<outline isEditable="true" isMoveable="true" text="Setup Wizard">
									<outline isEditable="true" isMoveable="true" text="General NetBeans settings">
										<outline isEditable="true" isMoveable="true" text="Set proxy server as necessary"/>
										<outline isEditable="true" isMoveable="true" text="SDI is windowing default, that's fine"/>
										<outline isEditable="true" isMoveable="true" text="Set web browser"/>
									</outline>
									<outline isEditable="true" isMoveable="true" text="Ready To Go">
										<outline isEditable="true" isMoveable="true" text="Click the Finish button"/>
									</outline>
									<outline isEditable="true" isMoveable="true" text="Welcome to NetBeans">
										<outline isEditable="true" isMoveable="true" text="Unchecked Show Welcome Screen on Startup widget"/>
										<outline isEditable="true" isMoveable="true" text="Click the Close button"/>
									</outline>
								</outline>
								<outline isEditable="true" isMoveable="true" text="Welcome to NetBeans">
									<outline isEditable="true" isMoveable="true" text="Unchecked Show Welcome Screen on Startup widget"/>
