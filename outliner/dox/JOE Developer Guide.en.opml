<outline isEditable="true" isMoveable="true" text="-------------------------------" isMoveableInherited="true" isEditableInherited="true"/>
<outline isEditable="true" isMoveable="true" text="Front End" isMoveableInherited="true" isEditableInherited="true">
	<outline isEditable="true" isMoveable="true" text="About JOE">
		<outline isEditable="true" isMoveable="true" text="Notable Features">
			<outline isEditable="true" isMoveable="true" text="Smooth Solid Feel"/>
			<outline isEditable="true" isMoveable="true" text="Standards-Respectful UI">
				<outline isEditable="true" isMoveable="true" text="Userland-inspired outline-editing UI">
					<outline isEditable="true" isMoveable="true" text="link to Userland outliner site[s]"/>
				</outline>
				<outline isEditable="true" isMoveable="true" text="Best-practices File/Edit/Search/Window/Help menu command sets"/>
			</outline>
			<outline isEditable="true" isMoveable="true" text="Multiple undo">
				<outline isEditable="true" isMoveable="true" text="limit [huge]: available memory"/>
			</outline>
			<outline isEditable="true" isMoveable="true" text="Highly Customizable">
				<outline isEditable="true" isMoveable="true" text="Application Preferences"/>
				<outline isEditable="true" isMoveable="true" text="User Preferences [forthcoming]"/>
				<outline isEditable="true" isMoveable="true" text="Document Preferences"/>
			</outline>
			<outline isEditable="true" isMoveable="true" text="Multi-User Friendly"/>
			<outline isEditable="true" isMoveable="true" text="Multiple Format Support">
				<outline isEditable="true" isMoveable="true" text="Text "/>
				<outline isEditable="true" isMoveable="true" text="XML"/>
				<outline isEditable="true" isMoveable="true" text="OPML">
					<outline isEditable="true" isMoveable="true" text="OPML 1.0"/>
					<outline isEditable="true" isMoveable="true" text="Userland node types and attributes"/>
					<outline isEditable="true" isMoveable="true" text="JOE node types and attributes"/>
				</outline>
				<outline isEditable="true" isMoveable="true" text="Palm pdf formats [forthcoming]">
					<outline isEditable="true" isMoveable="true" text="Brain Forest"/>
					<outline isEditable="true" isMoveable="true" text="Thought Manager"/>
				</outline>
			</outline>
			<outline isEditable="true" isMoveable="true" text="'net-Friendly">
				<outline isEditable="true" isMoveable="true" text="http support [forthcoming]"/>
				<outline isEditable="true" isMoveable="true" text="ftp support [forthcoming]"/>
				<outline isEditable="true" isMoveable="true" text="WebDav support [forthcoming]"/>
			</outline>
			<outline isEditable="true" isMoveable="true" text="OS-Agnostic">
				<outline isEditable="true" isMoveable="true" text="JOE is programmed in Java, using the Swing UI classes">
					<outline isEditable="true" isMoveable="true" text="JOE supports native platform look and feel [forthcoming when Sun gets this stuff stable]"/>
				</outline>
				<outline isEditable="true" isMoveable="true" text="JOE releases are available for these platforms">
					<outline isEditable="true" isMoveable="true" text="Windows"/>
					<outline isEditable="true" isMoveable="true" text="Linux/Unix [forthcoming]"/>
					<outline isEditable="true" isMoveable="true" text="Macintosh [forthcoming]"/>
				</outline>
			</outline>
			<outline isEditable="true" isMoveable="true" text="Open Source"/>
		</outline>
	</outline>
	<outline isEditable="true" isMoveable="true" text="About JOE Developer Guide [JDG]">
		<outline isEditable="true" isMoveable="true" text="This is a work in perpetual progress.">
			<outline isEditable="true" isMoveable="true" text="Empty/incomplete/missing nodes are generally not bugs, they're just things someone hasn't gotten 'round to thinking thru andor writing about completely yet"/>
		</outline>
		<outline isEditable="true" isMoveable="true" text="Wanna help ?? ">
			<outline isEditable="true" isMoveable="true" text="Send suggestions for nodes"/>
			<outline isEditable="true" isMoveable="true" text="Send content contributions">
				<outline isEditable="true" isMoveable="true" text="Study the JDG style, write up a node or two, stick the OPML up on the web, and send a link to it"/>
			</outline>
			<outline isEditable="true" isMoveable="true" text="Go here to send">
				<outline isEditable="true" isMoveable="true" text="http://JavaOutlineEditor.org/feedback"/>
			</outline>
		</outline>
	</outline>
