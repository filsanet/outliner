	<outline isEditable="true" isMoveable="true" text="Windows">
		<outline isEditable="true" isMoveable="true" text="Updating to Latest File Versions">
			<outline isEditable="true" isMoveable="true" text="Preparing for a general source update">
				<outline isEditable="true" isMoveable="true" text="Delete the .class files">
					<outline isEditable="true" isMoveable="true" text="They only produce ? update listings"/>
				</outline>
			</outline>
			<outline isEditable="true" isMoveable="true" text="Updating all project files">
				<outline isEditable="true" isMoveable="true" text="open a command line, and go to the project's root directory"/>
				<outline isEditable="true" isMoveable="true" text="give this command">
					<outline isEditable="true" isMoveable="true" text="cvs update"/>
				</outline>
			</outline>
			<outline isEditable="true" isMoveable="true" text="Updating specific project files"/>
		</outline>
		<outline isEditable="true" isMoveable="true" text="Debugging a NetBeans/Ant-built JOE">
			<outline text="Compile with the debug option">
				<outline text="example">
					<outline text="from stan's build.xml's compile node">
						<outline text="&lt;javac debug=&quot;true&quot; deprecation=&quot;false&quot; srcdir=&quot;.&quot;&gt;"/>
					</outline>
				</outline>
			</outline>
			<outline text="Invoke the runtime with debug hooks">
				<outline text="the hooks">
					<outline text="-Xdebug -Xnoagent  -Xrunjdwp:transport=dt_shmem,server=y,address=someName"/>
					<outline text="for further detail, look here">
						<outline text="http://debuggercore.netbeans.org/docs/VM-options.html"/>
					</outline>
				</outline>
				<outline text="example">
					<outline text="from stan's run.bat">
						<outline text="java -cp %CP% -Xincgc -Xdebug -Xnoagent -Xrunjdwp:transport=dt_shmem,server=y,address=joeDebug com.organic.maynard.outliner.Outliner en"/>
					</outline>
				</outline>
			</outline>
			<outline text="In NetBeans, attach the debugger ">
				<outline text="give the Debug:Attach... menu command"/>
				<outline text="set options as shown in this example">
					<outline text="http://JavaOutlineEditor.org/dox/images/debug-AttachToVM.gif"/>
				</outline>
			</outline>
			<outline text="That's it -- let's test it out">
				<outline text="set up a breakpoint">
					<outline text="Open up the file Outliner.java in the NetBeans Source Editor window"/>
					<outline text="Place the cursor in the first statement of the method Main"/>
					<outline text="Give the Debug:Add Breakpoint... menu command"/>
					<outline text="Fill in the fields in the Add Breakpoint dialog, and click OK">
						<outline text="example">
							<outline text="http://JavaOutlineEditor.org/dox/images/debug-AddBreakpoint.gif"/>
						</outline>
					</outline>
				</outline>
				<outline text="invoke the runtime with debug hooks">
					<outline text="see above"/>
				</outline>
				<outline text="in NetBeans, attach the debugger">
					<outline text="see above"/>
				</outline>
				<outline text="the app will break at your breakpoint"/>
			</outline>
			<outline text="To stop debugging">
				<outline text="in NetBeans, give the Debug:Finish menu command"/>
			</outline>
			<outline text="Thanks to">
				<outline text="Jan Jancura &lt;jan.jancura@czech.sun.com&gt;">
					<outline text="He sent the following info. The specific info had some problems, but the link he provided gave me the info I needed to fix those">
						<outline text="1) You would compile your sources with option -g (javac.exe -g ...)"/>
						<outline text="2) you would add -Xdebug -Xnoagent -Xrunjdwp:transport=dt_socket,address=2569,suspend=n options to your "/>
						<outline text="java.exe line in your batch file."/>
						<outline text="3) you can connect to running program: menu Debug / Attach / Default "/>
						<outline text="Debugger (JPDA) / SocketAttach / port = 2569"/>
						<outline text="see http://debuggercore.netbeans.org/docs/VM-options.html for more info, "/>
						<outline text="please."/>
					</outline>
				</outline>
			</outline>
		</outline>
