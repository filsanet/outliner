	<outline isEditable="true" isMoveable="true" text="ssh">
		<outline isEditable="true" isMoveable="true" text="How to avoid having to type in ssh password when command-lining a cvs command">
			<outline isEditable="true" isMoveable="true" text="aha !!! this one's figured out -- I just need to clean up the instructions">
				<outline isEditable="true" isMoveable="true" text="create a public key ">
					<outline isEditable="true" isMoveable="true" text="run ssh-keygen -C &quot;someUsefulIdentifier&quot;"/>
					<outline isEditable="true" isMoveable="true" text="if your machine is secure, use an empty passphrase">
						<outline isEditable="true" isMoveable="true" text="that way, you can just run cvs without having to continually enter a password or passphrase"/>
					</outline>
					<outline isEditable="true" isMoveable="true" text="sample">
						<outline isEditable="true" isMoveable="true" text="C:\Home\.ssh&gt;ssh-keygen -C JoeOnSourceForge"/>
						<outline isEditable="true" isMoveable="true" text="Initializing random number generator..."/>
						<outline isEditable="true" isMoveable="true" text="Generating p:  ..........................................++ (distance 880)"/>
						<outline isEditable="true" isMoveable="true" text="Generating q:  ................++ (distance 190)"/>
						<outline isEditable="true" isMoveable="true" text="Computing the keys..."/>
						<outline isEditable="true" isMoveable="true" text="Testing the keys..."/>
						<outline isEditable="true" isMoveable="true" text="Key generation complete."/>
						<outline isEditable="true" isMoveable="true" text="Enter file in which to save the key ($HOME/.ssh/identity):"/>
						<outline isEditable="true" isMoveable="true" text="Enter passphrase (empty for no passphrase):"/>
						<outline isEditable="true" isMoveable="true" text="Enter same passphrase again:"/>
						<outline isEditable="true" isMoveable="true" text="Your identification has been saved in c:\home/.ssh/identity."/>
						<outline isEditable="true" isMoveable="true" text="Your public key is:"/>
						<outline isEditable="true" isMoveable="true" text="99999999999999999999999999999999999999"/>
						<outline isEditable="true" isMoveable="true" text="99999999999999999999999999999999999999"/>
						<outline isEditable="true" isMoveable="true" text="99999999999999999999999999999999999999"/>
						<outline isEditable="true" isMoveable="true" text="99999999999999999999999999999999999999JoeOnSourceForge"/>
						<outline isEditable="true" isMoveable="true" text="Your public key has been saved in c:\home/.ssh/identity.pub"/>
					</outline>
				</outline>
				<outline isEditable="true" isMoveable="true" text="copy the public key to the clipboard">
					<outline isEditable="true" isMoveable="true" text="open up the public key file in a text editor">
						<outline isEditable="true" isMoveable="true" text="in our example, that's c:\home\.ssh\identity.pub"/>
					</outline>
					<outline isEditable="true" isMoveable="true" text="Select All and copy its contents"/>
				</outline>
				<outline isEditable="true" isMoveable="true" text="stick that data up on SourceForge">
					<outline isEditable="true" isMoveable="true" text="Log onto SourceForge"/>
					<outline isEditable="true" isMoveable="true" text="Go to your Account Maintenance page"/>
					<outline isEditable="true" isMoveable="true" text="At the bottom of the page, there's a section called Shell Account Information"/>
					<outline isEditable="true" isMoveable="true" text="There's a line like this">
						<outline isEditable="true" isMoveable="true" text="CVS/SSH Shared Authorized Keys: "/>
					</outline>
					<outline isEditable="true" isMoveable="true" text="Starts out with keys equal to zero"/>
					<outline isEditable="true" isMoveable="true" text="Click on Edit Keys"/>
					<outline isEditable="true" isMoveable="true" text="Paste in the key you copied"/>
					<outline isEditable="true" isMoveable="true" text="Click Okay"/>
				</outline>
				<outline isEditable="true" isMoveable="true" text="Wait 6 to 12 hours for the key to take effect"/>
				<outline isEditable="true" isMoveable="true" text="check it out">
					<outline isEditable="true" isMoveable="true" text="Open up a command line window"/>
					<outline isEditable="true" isMoveable="true" text="Give a CVS command"/>
					<outline isEditable="true" isMoveable="true" text="Your project gets added to the set of hosts"/>
					<outline isEditable="true" isMoveable="true" text="It looks like this">
						<outline isEditable="true" isMoveable="true" text="E:\stan's java projects\Joe\outliner\src\com\organic\maynard\outliner&gt;cvs update"/>
						<outline isEditable="true" isMoveable="true" text=""/>
						<outline isEditable="true" isMoveable="true" text="Host 'cvs.outliner.sourceforge.net' added to the list of known hosts."/>
						<outline isEditable="true" isMoveable="true" text="Password:"/>
						<outline isEditable="true" isMoveable="true" text="? .nbattrs"/>
						<outline isEditable="true" isMoveable="true" text="? stan's on-deck circle"/>
						<outline isEditable="true" isMoveable="true" text="cvs server: Updating ."/>
						<outline isEditable="true" isMoveable="true" text="M DocumentManager.java"/>
						<outline isEditable="true" isMoveable="true" text="cvs server: nothing known about JOEReturnCodes.java"/>
					</outline>
					<outline isEditable="true" isMoveable="true" text="open up a command line window"/>
					<outline isEditable="true" isMoveable="true" text="give a CVS command">
						<outline isEditable="true" isMoveable="true" text="example">
							<outline isEditable="true" isMoveable="true" text="E:\stan's java projects\Joe\outliner\dox&gt;cvs commit -m &quot;ongoing writing/editing/organizing&quot; &quot;JOE User Guide.en.opml&quot;"/>
						</outline>
					</outline>
					<outline isEditable="true" isMoveable="true" text="you'll be asked for a passphrase">
						<outline isEditable="true" isMoveable="true" text="example">
							<outline isEditable="true" isMoveable="true" text="Enter passphrase for RSA key 'hostname':"/>
						</outline>
					</outline>
				</outline>
			</outline>
		</outline>
