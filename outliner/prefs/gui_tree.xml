<?xml version="1.0"?>
<root id="outliner" class="com.organic.maynard.outliner.Outliner" title="JOE">
	<preferences id="prefs" class="com.organic.maynard.outliner.Preferences">
		<preference id="most_recent_open_dir" class="com.organic.maynard.outliner.PreferenceString" default="."/>
		<preference id="most_recent_save_dir" class="com.organic.maynard.outliner.PreferenceString" default="."/>
		<preference id="is_maximized" class="com.organic.maynard.outliner.PreferenceBoolean" default="false">
			<validator id="is_maximized_validator" class="com.organic.maynard.outliner.BooleanValidator">
			</validator>
		</preference>
		<preference id="time_zone_for_saving_dates" class="com.organic.maynard.outliner.PreferenceString" default="GMT"/>
		<preference id="renderer_widgit_cache_size" class="com.organic.maynard.outliner.PreferenceInt" default="40">
			<validator id="renderer_widgit_cache_size_validator" class="com.organic.maynard.outliner.IntRangeValidator" min="4" max="256" default="40">
			</validator>
		</preference>
		<preferencesframe id="preferences_frame" class="com.organic.maynard.outliner.PreferencesFrame" title="Preferences">
			<preferencepanel id="preferences_panel_editor" class="com.organic.maynard.outliner.PreferencesPanelEditor" title="Editor">
				<preference id="font_face" class="com.organic.maynard.outliner.PreferenceString" default="SansSerif"/>
				<preference id="font_size" class="com.organic.maynard.outliner.PreferenceInt" default="12">
					<validator id="font_size_validator" class="com.organic.maynard.outliner.IntRangeValidator" min="6" max="36" default="12">
					</validator>
				</preference>
				<preference id="line_wrap" class="com.organic.maynard.outliner.PreferenceString" default="words"/>
				<preference id="undo_queue_size" class="com.organic.maynard.outliner.PreferenceInt" default="100">
					<validator id="undo_queue_size_validator" class="com.organic.maynard.outliner.IntRangeValidator" min="0" max="100000" default="100">
					</validator>
				</preference>
				<preference id="show_line_numbers" class="com.organic.maynard.outliner.PreferenceBoolean" default="true">
					<validator id="show_line_numbers_validator" class="com.organic.maynard.outliner.BooleanValidator">
					</validator>
				</preference>
				<preference id="show_attributes" class="com.organic.maynard.outliner.PreferenceBoolean" default="false">
					<validator id="show_attributes_validator" class="com.organic.maynard.outliner.BooleanValidator">
					</validator>
				</preference>
			</preferencepanel>
			<preferencepanel id="preferences_panel_look_and_feel" class="com.organic.maynard.outliner.PreferencesPanelLookAndFeel" title="Look &amp; Feel">
				<preference id="desktop_background_color" class="com.organic.maynard.outliner.PreferenceColor" default="-16777216"/>
				<preference id="panel_background_color" class="com.organic.maynard.outliner.PreferenceColor" default="-1"/>
				<preference id="textarea_background_color" class="com.organic.maynard.outliner.PreferenceColor" default="-986896"/>
				<preference id="textarea_foreground_color" class="com.organic.maynard.outliner.PreferenceColor" default="-16777216"/>
				<preference id="selected_child_color" class="com.organic.maynard.outliner.PreferenceColor" default="-10987432"/>
				<preference id="line_number_color" class="com.organic.maynard.outliner.PreferenceColor" default="-1315861"/>
				<preference id="line_number_selected_color" class="com.organic.maynard.outliner.PreferenceColor" default="-2960686"/>
				<preference id="line_number_selected_child_color" class="com.organic.maynard.outliner.PreferenceColor" default="-2302756"/>
				<preference id="vertical_spacing" class="com.organic.maynard.outliner.PreferenceInt" default="2">
					<validator id="vertical_spacing_validator" class="com.organic.maynard.outliner.IntRangeValidator" min="0" max="99" default="2">
					</validator>
				</preference>
				<preference id="indent" class="com.organic.maynard.outliner.PreferenceInt" default="15">
					<validator id="indent_validator" class="com.organic.maynard.outliner.IntRangeValidator" min="1" max="99" default="15">
					</validator>
				</preference>
				<preference id="left_margin" class="com.organic.maynard.outliner.PreferenceInt" default="5">
					<validator id="left_margin_validator" class="com.organic.maynard.outliner.IntRangeValidator" min="0" max="99" default="5">
					</validator>
				</preference>
				<preference id="top_margin" class="com.organic.maynard.outliner.PreferenceInt" default="5">
					<validator id="top_margin_validator" class="com.organic.maynard.outliner.IntRangeValidator" min="0" max="99" default="5">
					</validator>
				</preference>
				<preference id="right_margin" class="com.organic.maynard.outliner.PreferenceInt" default="5">
					<validator id="right_margin_validator" class="com.organic.maynard.outliner.IntRangeValidator" min="0" max="99" default="5">
					</validator>
				</preference>
				<preference id="bottom_margin" class="com.organic.maynard.outliner.PreferenceInt" default="5">
					<validator id="bottom_margin_validator" class="com.organic.maynard.outliner.IntRangeValidator" min="0" max="99" default="5">
					</validator>
				</preference>
			</preferencepanel>
			<preferencepanel id="preferences_panel_open_and_save" class="com.organic.maynard.outliner.PreferencesPanelOpenAndSave" title="Open &amp; Save">
				<preference id="line_end" class="com.organic.maynard.outliner.PreferenceLineEnding"/>
				<preference id="open_encoding" class="com.organic.maynard.outliner.PreferenceString" default="ISO-8859-1"/>
				<preference id="save_encoding" class="com.organic.maynard.outliner.PreferenceString" default="ISO-8859-1"/>
				<preference id="open_format" class="com.organic.maynard.outliner.PreferenceString" default="Plaintext"/>
				<preference id="save_format" class="com.organic.maynard.outliner.PreferenceString" default="Plaintext"/>
			</preferencepanel>
			<preferencepanel id="preferences_panel_misc" class="com.organic.maynard.outliner.PreferencesPanelMisc" title="Misc.">
				<preference id="print_environment" class="com.organic.maynard.outliner.PreferenceBoolean" default="false">
					<validator id="print_environment_validator" class="com.organic.maynard.outliner.BooleanValidator">
					</validator>
				</preference>
				<preference id="new_doc_on_startup" class="com.organic.maynard.outliner.PreferenceBoolean" default="true">
					<validator id="new_doc_on_startup_validator" class="com.organic.maynard.outliner.BooleanValidator">
					</validator>
				</preference>
				<preference id="recent_files_list_size" class="com.organic.maynard.outliner.PreferenceInt" default="10">
					<validator id="recent_files_list_size_validator" class="com.organic.maynard.outliner.IntRangeValidator" min="0" max="99" default="10">
					</validator>
				</preference>
				<preference id="owner_name" class="com.organic.maynard.outliner.PreferenceString" default=""/>
				<preference id="owner_email" class="com.organic.maynard.outliner.PreferenceString" default=""/>
				<preference id="web_file_system" class="com.organic.maynard.outliner.PreferenceBoolean" default="false">
					<validator id="web_file_system_validator" class="com.organic.maynard.outliner.BooleanValidator">
					</validator>
				</preference>
				<preference id="web_file_url" class="com.organic.maynard.outliner.PreferenceString" default="http://localhost/outliner.php"/>
				<preference id="web_file_user" class="com.organic.maynard.outliner.PreferenceString" default=""/>
				<preference id="web_file_password" class="com.organic.maynard.outliner.PreferenceString" default=""/>
			</preferencepanel>
		</preferencesframe>
	</preferences>
	<menubar id="menubar" class="com.organic.maynard.outliner.OutlinerDesktopMenuBar">
		<menu id="file" class="com.organic.maynard.outliner.FileMenu" text="File">
			<menuitem id="new_file" class="com.organic.maynard.outliner.NewFileMenuItem" text="New" keybinding="N" keybindingmodifiers="control"/>
			<menuitem id="open_file" class="com.organic.maynard.outliner.OpenFileMenuItem" text="Open..." keybinding="O" keybindingmodifiers="control"/>
			<menu id="recent_file_list" class="com.organic.maynard.outliner.RecentFilesList" text="Open Recent Files">
			</menu>
			<separator/>
			<menuitem id="save" class="com.organic.maynard.outliner.SaveFileMenuItem" text="Save" keybinding="S" keybindingmodifiers="control"/>
			<menuitem id="save_all" class="com.organic.maynard.outliner.SaveAllFileMenuItem" text="Save All"/>
			<menuitem id="save_as" class="com.organic.maynard.outliner.SaveAsFileMenuItem" text="Save As..."/>
			<menuitem id="revert" class="com.organic.maynard.outliner.RevertFileMenuItem" text="Revert"/>
			<separator/>
			<menuitem id="close" class="com.organic.maynard.outliner.CloseFileMenuItem" text="Close" keybinding="W" keybindingmodifiers="control"/>
			<menuitem id="close_all" class="com.organic.maynard.outliner.CloseAllFileMenuItem" text="Close All"/>
			<separator/>
			<menuitem id="quit" class="com.organic.maynard.outliner.QuitMenuItem" text="Quit" keybinding="Q" keybindingmodifiers="control"/>
		</menu>
		<menu id="edit" class="com.organic.maynard.outliner.EditMenu" text="Edit">
			<menuitem id="undo" class="com.organic.maynard.outliner.UndoMenuItem" text="Undo" keybinding="Z" keybindingmodifiers="control"/>
			<menuitem id="redo" class="com.organic.maynard.outliner.RedoMenuItem" text="Redo" keybinding="Z" keybindingmodifiers="control shift"/>
			<separator/>
			<menuitem id="undo_all" class="com.organic.maynard.outliner.UndoAllMenuItem" text="Undo All"/>
			<menuitem id="redo_all" class="com.organic.maynard.outliner.RedoAllMenuItem" text="Redo All"/>
			<separator/>
			<menuitem id="cut" class="com.organic.maynard.outliner.CutMenuItem" text="Cut" keybinding="X" keybindingmodifiers="control"/>
			<menuitem id="copy" class="com.organic.maynard.outliner.CopyMenuItem" text="Copy" keybinding="C" keybindingmodifiers="control"/>
			<menuitem id="paste" class="com.organic.maynard.outliner.PasteMenuItem" text="Paste" keybinding="V" keybindingmodifiers="control"/>
			<separator/>
			<menuitem id="select_all" class="com.organic.maynard.outliner.SelectAllMenuItem" text="Select All" keybinding="A" keybindingmodifiers="control"/>
			<menuitem id="select_none" class="com.organic.maynard.outliner.SelectNoneMenuItem" text="Select None" keybinding="D" keybindingmodifiers="control"/>
			<separator/>
			<menuitem id="edit_document_settings" class="com.organic.maynard.outliner.EditDocumentSettingsMenuItem" text="Document Settings..."/>
			<separator/>
			<menuitem id="preferences" class="com.organic.maynard.outliner.PreferencesMenuItem" text="Preferences..."/>
			<menuitem id="macros" class="com.organic.maynard.outliner.MacrosMenuItem" text="Macros..."/>
		</menu>
		<menu id="outline" class="com.organic.maynard.outliner.OutlineMenu" text="Outline">
			<menuitem id="toggle_comment" class="com.organic.maynard.outliner.ToggleCommentMenuItem" text="Toggle Comment" keybinding="page_up"/>
			<separator/>
			<menuitem id="toggle_expansion" class="com.organic.maynard.outliner.ToggleExpansionMenuItem" text="Toggle Expansion" keybinding="page_down"/>
			<separator/>
			<menuitem id="expand_all_subheads" class="com.organic.maynard.outliner.ExpandAllSubheadsMenuItem" text="Expand All Subheads"/>
			<menuitem id="expand_everything" class="com.organic.maynard.outliner.ExpandEverythingMenuItem" text="Expand Everything"/>
			<separator/>
			<menuitem id="collapse_to_parent" class="com.organic.maynard.outliner.CollapseToParentMenuItem" text="Collapse To Parent"/>
			<menuitem id="collapse_everything" class="com.organic.maynard.outliner.CollapseEverythingMenuItem" text="Collapse Everything"/>
			<separator/>
			<menuitem id="move_up" class="com.organic.maynard.outliner.MoveUpMenuItem" text="Move Up" keybinding="up" keybindingmodifiers="shift"/>
			<menuitem id="move_down" class="com.organic.maynard.outliner.MoveDownMenuItem" text="Move Down" keybinding="down" keybindingmodifiers="shift"/>
			<menuitem id="move_left" class="com.organic.maynard.outliner.MoveLeftMenuItem" text="Move Left" keybinding="left" keybindingmodifiers="shift"/>
			<menuitem id="move_right" class="com.organic.maynard.outliner.MoveRightMenuItem" text="Move Right" keybinding="right" keybindingmodifiers="shift"/>
			<separator/>
			<menuitem id="promote" class="com.organic.maynard.outliner.PromoteMenuItem" text="Promote" keybinding="tab" keybindingmodifiers="shift"/>
			<menuitem id="demote" class="com.organic.maynard.outliner.DemoteMenuItem" text="Demote" keybinding="tab"/>
			<separator/>
			<menuitem id="merge" class="com.organic.maynard.outliner.MergeMenuItem" text="Merge" keybinding="M" keybindingmodifiers="control"/>
			<menuitem id="merge_with_spaces" class="com.organic.maynard.outliner.MergeWithSpacesMenuItem" text="Merge with Spaces" keybinding="M" keybindingmodifiers="control shift"/>
			<separator/>
			<menuitem id="hoist" class="com.organic.maynard.outliner.HoistMenuItem" text="Hoist"/>
			<menuitem id="dehoist" class="com.organic.maynard.outliner.DehoistMenuItem" text="De-Hoist"/>
			<menuitem id="dehoist_all" class="com.organic.maynard.outliner.DehoistAllMenuItem" text="De-Hoist All"/>
			<separator/>
			<menuitem id="toggle_attributes" class="com.organic.maynard.outliner.ToggleAttributesMenuItem" text="X" show="Show Attributes" hide="Hide Attributes" keybinding="T" keybindingmodifiers="control"/>
		</menu>
		<menu id="search" class="com.organic.maynard.outliner.SearchMenu" text="Search">
			<menuitem id="goto" class="com.organic.maynard.outliner.GoToMenuItem" text="Go to Line..." keybinding="G" keybindingmodifiers="control"/>
			<separator/>
			<menuitem id="find" class="com.organic.maynard.outliner.FindMenuItem" text="Find/Replace..." keybinding="F" keybindingmodifiers="control"/>
		</menu>
		<menu id="window" class="com.organic.maynard.outliner.WindowMenu" text="Window">
			<menuitem id="statistics" class="com.organic.maynard.outliner.StatisticsMenuItem" text="Document Statistics..."/>
			<separator/>
			<menuitem id="stack" class="com.organic.maynard.outliner.StackMenuItem" text="Stack"/>
			<separator/>
			<menuitem id="next_window" class="com.organic.maynard.outliner.NextWindowMenuItem" text="Next" keybinding="S" keybindingmodifiers="control shift"/>
			<menuitem id="prev_window" class="com.organic.maynard.outliner.PrevWindowMenuItem" text="Previous" keybinding="A" keybindingmodifiers="control shift"/>
			<separator/>
		</menu>
	</menubar>
</root>
